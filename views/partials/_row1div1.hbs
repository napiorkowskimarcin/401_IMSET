{{!-- <img class="img-responsive" src="img/one.png" alt="">   --}}
<div id="carouselExampleIndicators" class="carousel slide" data-interval="3200"  data-ride="carousel">
  {{!-- load dots to switch all of the slides --}}
  <ol class="carousel-indicators">
    <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
    <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
    <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
  </ol>
  {{!-- load divs with images --}}
  <div class="carousel-inner">
    <div class="carousel-item active">
      <img class="d-block img-responsive" src="img/one.png" alt="First slide">
    </div>
    <div class="carousel-item">
      <img class="d-block  img-responsive" src="img/one.png" alt="Second slide">
    </div>
    <div class="carousel-item">
      <img class="d-block  img-responsive" src="img/one.png" alt="Third slide">
    </div>
  </div>
</div>